{
  "name": "mj",
  "version": "1.0.2",
  "description": "mongodb-js tooling",
  "author": "mongodb-js <js@mongodb.com>",
  "bin": {
    "mj": "bin/mj.js",
    "mj-ci": "bin/mj-ci.js"
  },
  "dependencies": {
    "ampersand-model": "^6.0.2",
    "async": "^1.5.0",
    "chalk": "^1.1.1",
    "cli-color": "^1.1.0",
    "clui": "^0.3.1",
    "debug": "^2.2.0",
    "docopt": "^0.6.2",
    "download": "^4.4.1",
    "eslint-config-mongodb-js": "^1.0.4",
    "figures": "^1.4.0",
    "fs-extra": "^0.26.0",
    "glob": "^5.0.15",
    "is-mongodb-running": "^0.0.1",
    "joi": "^6.9.1",
    "khaos": "^0.9.3",
    "kill-mongodb": "0.0.1",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "mocha-evergreen-reporter": "^0.0.7",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.2",
    "mongodb-runner": "^3.1.11",
    "mongodb-version-manager": "^1.0.0",
    "multiline": "^1.0.2",
    "nsp": "^1.1.0",
    "rimraf": "^2.4.3",
    "run-series": "^1.1.4",
    "shelljs": "^0.5.3",
    "untildify": "^2.1.0",
    "update-notifier": "^0.5.0",
    "which": "^1.2.0",
    "winreg": "0.0.12",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "pre-commit": "^1.1.2"
  },
  "scripts": {
    "fmt": "mongodb-js-fmt bin/*.js lib/{*.js,**/*.js} test/{*.js,**/*.js}",
    "check": "./node_modules/.bin/mongodb-js-precommit bin/*.js lib/*.js",
    "test": "mocha",
    "ci": "node ./bin/mj-ci.js --verbose"
  },
  "homepage": "https://github.com/mongodb-js/mj",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/mj"
  },
  "contributors": [
    {
      "name": "Lucas Hrabovsky",
      "email": "lucas@mongodb.com"
    },
    {
      "name": "Thomas Rueckstiess",
      "email": "thomas@mongodb.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mongodb-js/mj/issues"
  },
  "precommit": [
    "check"
  ],
  "dependency-check": {
    "entries": [
      "bin/mj-ci.js"
    ],
    "ignore": [
      "cli-color",
      "download",
      "fs-extra",
      "is-mongodb-running",
      "joi",
      "khaos",
      "mkdirp",
      "mongodb-js-fmt",
      "nsp",
      "rimraf",
      "run-series",
      "shelljs",
      "winreg"
    ]
  }
}
